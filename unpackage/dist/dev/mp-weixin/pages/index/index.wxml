<view class="content"><video style="width:400px;height:800px;" id="video" src="{{dataList[0].url}}" autoplay="true" loop="{{isLoop}}" http-cache="{{true}}" controls="{{showControllers}}" show-fullscreen-btn="true" show-loading="true" play-btn-position="center" objectFit="contain" data-event-opts="{{[['loadedmetadata',[['loadData',['$event']]]],['tap',[['PauseVideo',['$event']]]],['fullscreenchange',[['disableIcon',['$event']]]],['ended',[['playEnd',['$event']]]]]}}" bindloadedmetadata="__e" bindtap="__e" bindfullscreenchange="__e" bindended="__e"></video><image hidden="{{!(!isPlay)}}" class="playBtn" src="../../static/play_1.png" data-event-opts="{{[['tap',[['playVideo',['$event']]]]]}}" bindtap="__e"></image><text value="{{dataList[0].name}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['dataList.__$n0']]]]]}}" bindinput="__e" style="margin:15px 0;">{{dataList[0].name}}</text><view class="memu"><switch checked="{{isAutoNext}}" data-event-opts="{{[['change',[['autoNext',['$event']]]]]}}" bindchange="__e"></switch>自动连播<button data-event-opts="{{[['tap',[['preVideo']]]]}}" bindtap="__e">上一个视频</button><button data-event-opts="{{[['tap',[['nextVideo']]]]}}" bindtap="__e">下一个视频</button><switch checked="{{isExRandom}}" data-event-opts="{{[['change',[['exRandom',['$event']]]]]}}" bindchange="__e"></switch>视频去重</view></view>